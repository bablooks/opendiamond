#if defined(RPC_HDR) || defined(RPC_XDR) || defined(MINIRPC)
%#include "rpc_preamble_xdr.h"
#else
#include "rpc_preamble.mx"
#endif

struct object_x {
    unsigned int search_id;	/* search identifier */
    opaque	 object<>;
    attribute_x  attrs<>;
};

struct credit_x {
    unsigned int credits;
};

clientmsgs {
    send_object(object_x) = 1;
}

servermsgs {
    update_credit(credit_x) = 1;
}

