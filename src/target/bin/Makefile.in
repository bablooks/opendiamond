ROOTDIR = @top_srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@

SUBDIRS = 

# list of everything in the directory
ALLFILES = $(wildcard *)

# items we know that we don't want to include
EXCLUDES = CVS Makefile Makefile.in

# The reduced set
INSTALL_BIN = $(filter-out $(EXCLUDES), $(ALLFILES))
                                                                                
CLEAN_OTHERS += ${INSTALL_BIN}
                                                                                
include $(ROOTDIR)/Makefile.template


