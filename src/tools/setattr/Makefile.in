ROOTDIR = @top_srcdir@
SRCDIR = @srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@


LIBS += -lodisk -ltools -lssl

#
# Compilation Targets
#

TARGETS = setattr
SRCS = setattr.c
OBJS = setattr.o


include $(ROOTDIR)/Makefile.template



#
#

SETATTR_OBJS = $(OBJS) 
setattr: $(SETATTR_OBJS)
