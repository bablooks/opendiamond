ROOTDIR = @top_srcdir@
SRCDIR = @srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@


LIBS += -lodisk -ltools -lpthread -llog -ldctl -lssl -lcrypto -ldconfig

#
# Compilation Targets
#

TARGETS = setattr
SRCS = setattr.c
OBJS = setattr.o


include $(OBJROOT)/Makefile.template



#
#

SETATTR_OBJS = $(OBJS) 
setattr: $(SETATTR_OBJS)
	$(CC) $(CFLAGS) -o setattr $(SETATTR_OBJS) $(LIBDIR) $(LIBS)

