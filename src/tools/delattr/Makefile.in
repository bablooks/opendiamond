ROOTDIR = @top_srcdir@

INCDIR += -I../../lib/libtools
INCDIR += -I../../lib/libsearchlet
INCDIR += -I../../lib/libodisk
LIBDIR += -L../../lib/libodisk -L../../lib/libtools
LIBS += -lodisk -ltools -lssl

#
# Compilation Targets
#
SRCS = delattr.c
OBJS = delattr.o 
PROG = delattr



include $(ROOTDIR)/Makefile.template



#
#

DELATTR_OBJS = ${OBJS} 
delattr: $(DELATTR_OBJS)
	$(CC) $(CFLAGS) -o delattr $(DELATTR_OBJS) $(LIBDIR) $(LIBS)



