ROOTDIR = @top_srcdir@
SRCDIR = @srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@

LIBS += -lodisk -ltools -lpthread -llog -ldctl -lssl -lcrypto -ldconfig

#
# Compilation Targets
#
SRCS = delattr.c
OBJS = delattr.o 
PROG = delattr



include $(OBJROOT)/Makefile.template



#
#

DELATTR_OBJS = ${OBJS} 
delattr: $(DELATTR_OBJS)
	$(CC) $(CFLAGS) -o delattr $(DELATTR_OBJS) $(LIBDIR) $(LIBS)



