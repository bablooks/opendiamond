ROOTDIR = @top_srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@


INCDIR += -I../../lib/libod -I../../lib/libsearchlet
INCDIR += -I../../lib/libodisk
OBJS = group_count.o
SRCS = group_count.c
LIBS += -lodisk -ltools -lpthread
LIBDIR += -L../../lib/libod -L../../lib/libodisk -L../../lib/libtools

TARGETS = group_count

include $(ROOTDIR)/Makefile.template 


.c.o:
	$(CC) $(CFLAGS) $(INCDIR) -c -o $*.o $<


group_count: group_count.o 
	$(CC) $(CFLAGS) -o group_count group_count.o $(LIBDIR) $(LIBS) 
