ROOTDIR = @top_srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@


INCDIR += -I../../lib/libtools
INCDIR += -I../../lib/libsearchlet
INCDIR += -I../../lib/libodisk
LIBDIR += -L../../lib/libodisk -L../../lib/libtools
LIBS += -lodisk -ltools -lssl
#
# Compilation Targets
#

TARGETS = initattr
SRCS = initattr.c
OBJS = initattr.o

include $(ROOTDIR)/Makefile.template



#
#

INITATTR_OBJS = $(OBJS) 
initattr: $(INITATTR_OBJS)
	$(CC) $(CFLAGS) -o initattr $(INITATTR_OBJS) $(LIBDIR) $(LIBS)

