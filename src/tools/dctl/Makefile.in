ROOTDIR = @top_srcdir@


INCDIR += -I../../lib/libsearchlet
INCDIR += -I../../lib/libdctl
INCDIR += -I../../lib/libtools

LIBDIR += -L../../lib/libtools

LIBS += -ltools

OBJS = dctl.o
SRCS = dctl.c

TARGETS = dctl 

SHARED_BIN = dctl 


include $(ROOTDIR)/Makefile.template

