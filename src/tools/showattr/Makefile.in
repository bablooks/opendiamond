ROOTDIR = @top_srcdir@
SRCDIR = @srcdir@
VPATH = @srcdir@
OBJROOT = @OBJROOT@

LIBS += -lodisk -ltools -lpthread -llog -ldctl -lssl -lcrypto -ldconfig

#
# Compilation Targets
#

TARGETS = showattr
SRCS = showattr.c
OBJS = showattr.o

include $(OBJROOT)/Makefile.template



SHOWATTR_OBJS = $(OBJS) 
showattr: $(SHOWATTR_OBJS)
	$(CC) $(CFLAGS) -o showattr $(SHOWATTR_OBJS) $(LIBDIR) $(LIBS)


