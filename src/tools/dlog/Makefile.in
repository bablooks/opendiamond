ROOTDIR = @top_srcdir@


INCDIR += -I../../lib/libtools
INCDIR += -I../../lib/libsearchlet
INCDIR += -I../../lib/libodisk
INCDIR += -I../../lib/liblog
LIBDIR += -L../../lib/libodisk
LIBDIR += -L../../lib/libtools
LIBS += -ltools


DLOG_OBJS = dlog.o
DLOG_SRCS = dlog.c

TEST_OBJS = test.o
TEST_SRCS = test.c

SRCS = $(DLOG_SRCS) $(TEST_SRCS)
OBJS = $(DLOG_OBJS) $(TEST_OBJS)

TARGETS = dlog test

SHARED_BIN = dlog 


include $(ROOTDIR)/Makefile.template



#
#

dlog: $(DLOG_OBJS)
	$(CC) $(CFLAGS) -o dlog $(DLOG_OBJS) $(LIBDIR) $(LIBS)

test: $(TEST_OBJS)
	$(CC) $(CFLAGS) -o test $(TEST_OBJS) $(LIBDIR) $(LIBS)

